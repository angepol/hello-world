<%= form_for @journey do |f| %>
<%= f.label :name %>
<%= f.text_field :name, :required => true %>

<%= f.label :countries%>
<%= f.text_field :countries %>

<%= f.label :departure_date %>
<%= f.text_field :departure_date %>

<%= f.label :return_date %>
<%= f.text_field :return_date %>

<%= f.label :image %>
<%= f.url_field :image %>

<%= f.submit %>
<% end %>
